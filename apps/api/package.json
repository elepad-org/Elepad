{
  "name": "@elepad/api",
  "version": "0.1.0",
  "private": true,
  "main": "src/app.ts",
  "types": "src/app.ts",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "node dist/src/server.js",
    "build": "tsc && tsx src/emit-openapi.ts",
    "build:edge": "node esbuild.edge.js",
    "dev": "tsx watch src/server.ts",
    "lint": "eslint . && tsc --noEmit",
    "clean": "rm -rf dist openapi.json",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.0",
    "@supabase/supabase-js": "^2.55.0",
    "dotenv": "^17.2.1",
    "hono": "^4.6.0",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "tsx": "^4.19.1",
    "typescript": "~5.8.3"
  },
  "packageManager": "npm@11.5.2"
}
