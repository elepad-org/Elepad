{
  "name": "elepad",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "start": "turbo run start",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write . --ignore-path .gitignore"
  },
  "devDependencies": {
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^9.1.0",
    "orval": "^7.11.2",
    "prettier": "^3.1.1",
    "turbo": "2.5.0"
  },
  "packageManager": "npm@11.5.2",
  "engines": {
    "node": ">=20"
  }
}
