{
  "name": "elepad",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "turbo run start",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write . --ignore-path .gitignore"
  },
  "devDependencies": {
    "prettier": "^3.1.1",
    "turbo": "2.5.0"
  },
  "packageManager": "npm@11.5.2",
  "engines": {
    "node": ">=20"
  }
}
